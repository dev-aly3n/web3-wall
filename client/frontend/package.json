    "react-dom": "^18"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@cspell/eslint-plugin": "^6.30.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
